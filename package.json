{
  "name": "redux-mocha-test-generators",
  "version": "1.0.0",
  "description": "A collection of methods to generate tests for redux actions, reducers, etc.",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "test-watch": "jest --watch",
    "test": "nyc --reporter=lcov npm run test-mocha",
    "lint": "eslint src/** --ext .js",
    "lint-examples": "eslint examples/** --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesalerno84/redux-mocha-test-generators.git"
  },
  "keywords": [
    "redux",
    "test",
    "mocha"
  ],
  "author": "Jeff Salerno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesalerno84/redux-mocha-test-generators/issues"
  },
  "homepage": "https://github.com/jesalerno84/redux-mocha-test-generators#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "codeclimate-test-reporter": "^0.5.0",
    "eslint": "^5.3.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0"
  },
  "dependencies": {
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0"
  }
}
