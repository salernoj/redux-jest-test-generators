{
  "name": "redux-mocha-test-generators",
  "version": "1.0.0",
  "description": "A collection of methods to generate tests for redux actions, reducers, etc.",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "test-examples": "mocha examples --compilers js:babel-register",
    "test-mocha": "mocha --compilers js:babel-register",
    "test": "nyc --reporter=lcov npm run test-mocha",
    "lint": "eslint src/** --ext .js",
    "lint-examples": "eslint examples/** --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesalerno84/redux-mocha-test-generators.git"
  },
  "keywords": [
    "redux",
    "test",
    "mocha"
  ],
  "author": "Jeff Salerno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesalerno84/redux-mocha-test-generators/issues"
  },
  "homepage": "https://github.com/jesalerno84/redux-mocha-test-generators#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^3.3.1",
    "jest": "^23.5.0",
    "proxyquire": "^1.7.10",
    "redux": "^3.5.2",
    "redux-mock-store": "^1.1.4",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.5"
  }
}
